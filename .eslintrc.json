{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "import/no-extraneous-dependencies":"off",
        "no-underscore-dangle" :"off",
        "no-restricted-globals" :"off",
        "no-console" :"off",
        "no-alert" :"off"
    }
}
