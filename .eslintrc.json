{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "import/no-extraneous-dependencies":"off",
        "no-underscore-dangle" :"off",
        "no-restricted-globals" :"off",
        "no-console" :"off",
        "no-alert" :"off",
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "no-new": 0,
        "no-undef": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0
    }
}
